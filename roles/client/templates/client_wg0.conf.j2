[Interface]
Address = 10.0.1.8/32
# load privatekey from file
PostUp = wg set %i private-key /etc/wireguard/privatekey
# ping Tunnel
PostUp = ping -c1 10.0.1.1

[Peer]
PublicKey = {{ server_pubkey }}
Endpoint = {{ hostvars['TEST'].ansible_host }}:50499
AllowedIPs = 10.0.1.1/24
PersistentKeepalive = 25
